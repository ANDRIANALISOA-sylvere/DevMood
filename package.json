{
  "name": "devmood",
  "displayName": "DevMood",
  "description": "Emotional Analytics for VS Code Real-time mood detection for developers",
  "version": "1.0.0",
  "publisher": "A. Joséphin Sylvère",
  "repository": "https://github.com/ANDRIANALISOA-sylvere/DevMood",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
